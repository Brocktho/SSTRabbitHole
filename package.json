{
  "name": "my-remix-app-5554",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "generate": "drizzle-kit generate:mysql --out migrations-folder",
    "build": "remix build",
    "migrate": "npx ts-node ./app/db/migrate",
    "db:push": "drizzle-kit push:mysql --config ./drizzle.config.ts",
    "dev": "sst bind remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev",
    "deploy": "NODE_ENV=production sst deploy --stage prod"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@planetscale/database": "^1.7.0",
    "@remix-run/css-bundle": "^1.17.1",
    "@remix-run/node": "^1.17.1",
    "@remix-run/react": "^1.17.1",
    "@remix-run/serve": "^1.17.1",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.27.0",
    "drizzle-zod": "^0.4.4",
    "isbot": "^3.6.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-typedjson": "^0.1.7",
    "tiny-invariant": "^1.3.1",
    "ulid": "^2.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "^1.17.1",
    "@remix-run/eslint-config": "^1.17.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.40.1",
    "@types/node": "^18.16.17",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "aws-cdk-lib": "2.79.1",
    "binode": "^1.0.5",
    "constructs": "10.1.156",
    "cookie": "^0.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.14.0",
    "drizzle-kit": "^0.19.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "happy-dom": "^9.20.3",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "sst": "^2.15.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.32.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
